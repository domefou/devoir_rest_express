{
    "version": 2,
    "builds": [
        {
            "src": "./bin/www.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "./bin/www.js"
        }
    ]
}