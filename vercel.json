{
    "version": 2,
    "builds": [
        {
            "src": "bin/www.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "bin/www.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "SECRET_KEY": "@secret_key",
        "URL_MONGO": "@url_mongo",
        "APP_NAME": "@app_name"
    }
}